
{
    "name": "manyrus/sms-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SmsBundle",
    "keywords": ["sms", "epochta", "sms.ru"],
    "homepage": "https://github.com/manyrus/SmsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "manyrus",
            "email": "manyrus@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "doctrine/doctrine-bundle": "*",
        "sensio/buzz-bundle": "dev-master"
    },
    "require-dev":{
        "phpunit/phpunit": "3.7.*"
    },

    "autoload": {
        "psr-0": { "Manyrus\\SmsBundle": "" }
    },
    "target-dir": "Manyrus/SmsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}